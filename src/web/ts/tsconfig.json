{
    "compilerOptions": {
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "removeComments": false,
        "preserveConstEnums": true,
        "skipLibCheck": true,
        "sourceMap": false,
        "lib": [ "dom", "es5", "scripthost", "es2015.promise" ]
    },
    "exclude": [
        "slot-action.ts"
    ]
}
